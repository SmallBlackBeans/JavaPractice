<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hanxiaocu.mybatis.db1.dao.PersonDao">
    <select id="findPersonByName" parameterType="String" resultType="com.hanxiaocu.mybatis.entity.Person">
        SELECT  * FROM person WHERE name = #{name}
    </select>
</mapper>